  dct:rights "copyright CSIRO 2009, 2016, 2018. All rights reserved."@en ;
  dct:source "National Committee on Soil and Terrain (2009), 'Australian soil and land survey field handbook (3rd edn).' (CSIRO Publishing: Melbourne)"@en ;
  reg:notation "location"@en ;
  ui:hierarchyChildProperty skos:member ;
  ui:hierarchyRootProperty ui:topMemberOf ;
  rdfs:label "Location classifiers"@en ;
  rdfs:seeAlso <http://www.publish.csiro.au/nid/22/pid/5230.htm> ;
  owl:imports dc: ;
  owl:imports <http://purl.org/linked-data/registry> ;
  owl:imports ui: ;
  owl:imports sosa: ;
  skos:changeNote """Add explicit identifiers using 

CONSTRUCT {
	?c dct:identifier ?id.
}
WHERE {
	?c a skos:Collection ;
	BIND ( strdt( str(?c) , xsd:anyURI ) as ?id )
}
"""@en ;
