.
asc:H
  rdf:type soil:Family ;
  rdf:type skos:Concept ;
  skos:definition "20 - <50 %" ;
  skos:notation "H" ;
  skos:prefLabel "Moderately gravelly" ;
.
asc:HA
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a melanic horizon and a B horizon in which at least the lower part is sodic." ;
  skos:prefLabel "Melanic-sodic" ;
.
asc:HB
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which the major part of the B2 horizon is mottled and at least the lower part of the B horizon is sodic." ;
  skos:prefLabel "Mottled-sodic" ;
.
asc:HC
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a ferric horizon within the solum and a B horizon in which at least the lower part issodic." ;
  skos:prefLabel "Ferric-sodic" ;
.
asc:HD
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which the organic materials directly overlie fragmental material such as gravel, cobbles or stones in which the interstices are filled or partially filled with organic material." ;
  skos:prefLabel "Ruaceous" ;
.
asc:HE
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which the major part of the solum below 0.5 m is calcareous and the major part of the upper 0.5 m of the solum is mottled." ;
  skos:prefLabel "Endocalcareous-mottled" ;
.
asc:HF
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which the gravel consists dominantly of little-weathered tephric materials." ;
  skos:prefLabel "Tephric" ;
.
asc:HG
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which the upper 0.5 m (or less if the profile is shallower) consists dominantly of carbic materials." ;
  skos:prefLabel "Carbic" ;
.
asc:HH
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which at least the upper 0.5m of the profile consists dominantly of unconsolidated mineral materials which are gravelly (>10% >2mm). The gravel may occur as distinct layers, or be uniformly or irregularly distributed." ;
  skos:prefLabel "Clastic" ;
.
asc:HI
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which the gravel consists of usually unsorted material with a wide size range. It is largely colluvial mass movement debris, including scree and talus, and landslide, mudflow and creep deposits." ;
  skos:prefLabel "Colluvic" ;
.
asc:HJ
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Other soils in which the unconsolidated mineral materials are dominated by gravel which mostly consists of rock or mineral fragments." ;
  skos:prefLabel "Lithosolic" ;
.
asc:HK
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which the upper 0.1m of the profile consists of dominantly soft, finely divided carbonate (visual estimate), and/or contains more than 40%2 (by analysis) of soft, finely divided carbonate." ;
  skos:prefLabel "Supravescent" ;
.
asc:HL
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Other soils with sulfidic materials in the A or near surface horizons." ;
  skos:prefLabel "Episulfidic" ;
.
asc:HM
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with sulfidic materials in the A or near surface horizons and which directly overlie a calcrete pan." ;
  skos:prefLabel "Episulfidic-petrocalcic" ;
.
asc:HN
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a densipan present in the A2 horizon and a thin ironpan which occurs within or directly underlying the B horizon." ;
  skos:prefLabel "Densic-placic" ;
.
asc:HO
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which the major part of the B2 horizon is strongly acid and at least the lower part is sodic" ;
  skos:prefLabel "Acidic-sodic" ;
.
