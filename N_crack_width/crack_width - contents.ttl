@prefix crack_width: <http://registry.it.csiro.au/def/soils/crack-width/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema> .

<coarse>
  rdf:type skos:Concept ;
  rdfs:label "Coarse" ;
  <http://www.w3.org/2001/XMLSchema#maxExclusive> "20"^^<http://www.w3.org/2001/XMLSchema#string> ;
  <http://www.w3.org/2001/XMLSchema#minInclusive> "10"^^<http://www.w3.org/2001/XMLSchema#string> ;
  skos:definition "Coarse, 10-20mm" ;
  skos:notation "3" ;
  skos:prefLabel "Coarse"@en ;
.
<extremely-coarse>
  rdf:type skos:Concept ;
  rdfs:label "Extremely Coarse" ;
  <http://www.w3.org/2001/XMLSchema#maxExclusive> "100"^^<http://www.w3.org/2001/XMLSchema#string> ;
  <http://www.w3.org/2001/XMLSchema#minInclusive> "50"^^<http://www.w3.org/2001/XMLSchema#string> ;
  skos:definition "Extremely coarse, >50mm" ;
  skos:notation "5" ;
  skos:prefLabel "Extremely Coarse"@en ;
.
<fine>
  rdf:type skos:Concept ;
  rdfs:label "Fine" ;
  <http://www.w3.org/2001/XMLSchema#maxExclusive> "5"^^<http://www.w3.org/2001/XMLSchema#string> ;
  <http://www.w3.org/2001/XMLSchema#minInclusive> "0"^^<http://www.w3.org/2001/XMLSchema#string> ;
  skos:definition "Fine, <5mm" ;
  skos:notation "1" ;
  skos:prefLabel "Fine"@en ;
.
<medium>
  rdf:type skos:Concept ;
  rdfs:label "Medium" ;
  <http://www.w3.org/2001/XMLSchema#maxExclusive> "10"^^<http://www.w3.org/2001/XMLSchema#string> ;
  <http://www.w3.org/2001/XMLSchema#minInclusive> "5"^^<http://www.w3.org/2001/XMLSchema#string> ;
  skos:definition "Medium, 5-10mm" ;
  skos:notation "2" ;
  skos:prefLabel "Medium"@en ;
.
<very-coarse>
  rdf:type skos:Concept ;
  rdfs:label "Very Coarse" ;
  <http://www.w3.org/2001/XMLSchema#maxExclusive> "50"^^<http://www.w3.org/2001/XMLSchema#string> ;
  <http://www.w3.org/2001/XMLSchema#minInclusive> "20"^^<http://www.w3.org/2001/XMLSchema#string> ;
  skos:definition "Very coarse, 20-50mm" ;
  skos:notation "4" ;
  skos:prefLabel "Very Coarse"@en ;
.
