  dct:modified "2017-05-19"^^xsd:date ;
  dct:source """The Australian Soil Classification, second edition (2016). 
CSIRO Publishing, Melbourne, Australia""" ;
  rdfs:comment "This ontology provides sets of classifiers defined for the Australian Soil Classification. It uses classes for and a structure from the general soil-classification ontology.  " ;
  rdfs:seeAlso <http://www.clw.csiro.au/aclep/asc_re_on_line_V2/soilkey.htm> ;
  owl:imports <http://registry.it.csiro.au/def/soils/soil-classification> ;
  skos:editorialNote "Work in progress" ;
.
asc:A
  rdf:type soil:Family ;
  rdf:type skos:Concept ;
  skos:definition "< 0.1 m A-horizon thickness" ;
  skos:notation "A" ;
  skos:prefLabel "Thin" ;
.
asc:AA
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "The dominant colour class in the major part of the upper 0.5 m of the solum (or the major part of the entire solum if it is less than 0.5 m thick) is red." ;
  skos:prefLabel "Red" ;
.
asc:AB
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "The dominant colour class is brown." ;
  skos:prefLabel "Brown" ;
.
asc:AC
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "The dominant colour class is yellow." ;
  skos:prefLabel "Yellow" ;
.
asc:AD
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "The dominant colour class is grey." ;
  skos:prefLabel "Grey" ;
.
asc:AE
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "The dominant colour class is black." ;
  skos:prefLabel "Black" ;
.
asc:AF
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which the major part of the B2 horizon is dystrophic." ;
  skos:prefLabel "Dystrophic" ;
.
asc:AG
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which the major part of the B2 horizon is mesotrophic." ;
  skos:prefLabel "Mesotrophic" ;
.
asc:AH
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which the major part of the B2 horizon is eutrophic but the B and BC horizons are not calcareous." ;
  skos:prefLabel "Eutrophic" ;
.
asc:AI
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  rdfs:comment """DE, FE, HY, KA  Subgroup: Other soils with a B2 horizon in which the major part is strongly acid.
OR Great Group: Soils in which the major part of the organic materials is strongly acid.
RU Subgroup: The major part of the soil materials is strongly acid.
TE Subgroup: Soils in which the major part of the solum is strongly acid.""" ;
  skos:definition "DE, FE, HY, KA  Subgroup: Other soils with a B2 horizon in which the major part is strongly acid." ;
  skos:definition "OR Great Group: Soils in which the major part of the organic materials is strongly acid." ;
  skos:definition "RU Subgroup: The major part of the soil materials is strongly acid." ;
  skos:definition "TE Subgroup: Soils in which the major part of the solum is strongly acid." ;
  skos:notation "AI" ;
  skos:prefLabel "Acidic" ;
.
asc:AJ
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which the major part of the B2 horizon is strongly acid and mottled." ;
  skos:prefLabel "Acidic-mottled" ;
.
asc:AK
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils which have andic properties and has formed in basaltic tephric materials that may be visibly stratified." ;
  skos:prefLabel "Andic" ;
.
asc:AL
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with free drainage, ie. rapidly drained. There is no restriction to through drainage in the B horizon or within the substrate. There is no perching of water within the B horizon or saturation due to a high ground water table. The B horizons are weakly coherent and porous. They are often brightly coloured, and lack evidence of seasonal reduction." ;
  skos:prefLabel "Aeric" ;
.
asc:AM
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with long-term saturation in the B horizons. The saturation may be caused as in the semiaquic soils but the duration is of the order of months. The period of saturation is sufficient to reduce most iron compounds and move them out of the B horizon, hence Bh horizons are usually prominent." ;
  skos:prefLabel "Aquic" ;
.
asc:AN
  rdf:type soil:Order ;
  rdf:type skos:Concept ;
  skos:definition "Soils resulting from human activity" ;
  skos:prefLabel "Anthroposols" ;
.
asc:AO
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which at least the upper 0.5 m of the profile is not or only slightly gravelly (<10% >2mm) " ;
  skos:prefLabel "Arenic" ;
.
asc:AP
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with an argic horizon within the B horizon." ;
  skos:prefLabel "Argic" ;
.
asc:AQ
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "The soil materials to a depth of 0.5 m are dominantly clay-sized." ;
  skos:prefLabel "Argillaceous" ;
.
asc:AR
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which the major part of the organic materials is not calcareous but is not strongly acid." ;
  skos:prefLabel "Basic" ;
.
asc:AS
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a bauxitic horizon within the B horizon." ;
  skos:prefLabel "Bauxitic" ;
.
asc:AT
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a conspicuously bleached A2 horizon." ;
  skos:prefLabel "Bleached" ;
.
asc:AU
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a conspicuously bleached A2 horizon and a B2 horizon in which the major part is strongly acid." ;
  skos:prefLabel "Bleached-acidic" ;
.
asc:AV
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a conspicuously bleached A2 horizon and a ferric horizon within the solum." ;
  skos:prefLabel "Bleached-ferric" ;
.
asc:AW
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a conspicuously bleached A2 horizon, and are underlain within 0.5 m of the surface by a calcrete pan ; hard unweathered rock or other hard materials; or partially weathered or decomposed rock or saprolite." ;
  skos:prefLabel "Bleached-leptic" ;
.
asc:AX
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a conspicuously bleached A2 horizon and a B2 horizon in which the major part has an exchangeable Ca/Mg ratio of less than 0.1." ;
  skos:prefLabel "Bleached-magnesic" ;
.
asc:AY
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a conspicuously bleached A2 horizon and a manganic horizon within the solum." ;
  skos:prefLabel "Bleached-manganic" ;
.
asc:AZ
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a conspicuously bleached A2 horizon and the major part of the B2 horizon is mottled." ;
  skos:prefLabel "Bleached-mottled" ;
.
asc:B
  rdf:type soil:Family ;
  rdf:type skos:Concept ;
  skos:definition "0.1 - < 0.3 m A-horizon thickness" ;
  skos:notation "B" ;
  skos:prefLabel "Medium" ;
.
asc:BA
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a conspicuously bleached A2 horizon and a B horizon in which at least the lower part is sodic." ;
  skos:prefLabel "Bleached-sodic" ;
.
asc:BB
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a conspicuously bleached A2 horizon and a B horizon in which at least 0.3m has vertic properties." ;
  skos:prefLabel "Bleached-vertic" ;
.
asc:BC
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which the major part of the organic materials is calcareous." ;
  skos:prefLabel "Calcareous" ;
.
asc:BD
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Other soils with a calcareous horizon." ;
  skos:prefLabel "Calcic" ;
.
asc:BE
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Other soils with a peaty, humose, melacic or melanic horizon. A conspicuously bleached A2 horizon is not present." ;
  skos:prefLabel "Chernic" ;
.
asc:BF
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a peaty, humose, melacic or melanic horizon, and are underlain within 0.5 m of the surface by a calcrete pan; hard unweathered rock or other hard materials; or partially weathered or decomposed rock or saprolite. An unbleached A2 horizon may be present between the dark surface horizons and the substrate material." ;
  skos:prefLabel "Chernic-leptic" ;
.
asc:BG
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Other soils with a clear or abrupt textural B horizon and the pH in the major part of the upper 0.2 m of the B2 horizon is not strongly acid." ;
  skos:prefLabel "Chromosolic" ;
.
asc:BH
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a massive or weakly structured surface crusty horizon 0.03 m or less thick, often of lighter texture (lower clay content) than the underlying pedal clay (blocky or polyhedral) which is not self-mulching." ;
  skos:prefLabel "Crusty" ;
.
asc:BI
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Other soils with a densipan present in the A2 horizon." ;
  skos:prefLabel "Densic" ;
.
asc:BJ
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a red-brown hardpan either within or directly underlying the B horizon." ;
  skos:prefLabel "Duric" ;
.
asc:BK
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which the upper 0.2 m of the B2 horizon (or the B2 horizon if it is less than 0.2 m thick) has a strong blocky or polyhedral structure in which average ped size is usually in the range of 5-20 mm. There is very weak adhesion between peds (when dry it is very easy to insert a spade into the upper B2 horizon). Salt contents are usually high, resulting in weak dry strength and a bulk density of about 1.3 t m-3 or less. In some soils the B2 horizons may be weakly subplastic. A common feature (but not diagnostic) of the overlying A horizons is the presence of a band of vesicular pores near the surface or on the underside of any surface flake." ;
  skos:prefLabel "Pedaric" ;
.
asc:BL
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Other soils in which the major part of the solum below 0.5 m is strongly acid." ;
  skos:prefLabel "Endoacidic" ;
.
asc:BN
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Other soils in which the upper 0.1 m of the solum is sodic." ;
  skos:prefLabel "Episodic" ;
.
asc:BP
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Other soils in which some subhorizon of the solum below 0.5 m has an ESP of 15 or greater." ;
  skos:prefLabel "Endohypersodic" ;
.
asc:BR
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Other soils in which some subsurface horizon within the upper 0.5 m of the solum has an ESP of 15 or greater." ;
  skos:prefLabel "Epihypersodic" ;
.
asc:BT
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils of the extratidal zone (usually supporting grassland). Tidal inundation is infrequent and achieved only by exceptional storm or cyclonic tides (above high spring tides). Freshwater inundation is seasonally common." ;
  skos:prefLabel "Extratidal" ;
.
asc:BU
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a ferric horizon within the B horizon." ;
  skos:prefLabel "Ferric" ;
.
asc:BV
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "The soil materials to a depth of 0.5 m are dominantly sand-sized." ;
  skos:prefLabel "Arenaceous" ;
.
asc:BW
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which the organic materials are dominated (about 75% by volume) by fibric peat." ;
  skos:prefLabel "Fibric" ;
.
asc:BX
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which the gravel consists dominantly of mostly rounded materials that have been transported by streams or by wave action." ;
  skos:prefLabel "Fluvic" ;
.
asc:BY
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a weakly coherent B horizon." ;
  skos:prefLabel "Fragic" ;
.
asc:BZ
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Other soils with a gypsic horizon within the B or BC horizon." ;
  skos:prefLabel "Gypsic" ;
.
asc:C
  rdf:type soil:Family ;
  rdf:type skos:Concept ;
  skos:definition "0.3 - 0.6 m A-horizon thickness" ;
  skos:notation "C" ;
  skos:prefLabel "Thick" ;
.
asc:CA
  rdf:type soil:Order ;
  rdf:type skos:Concept ;
  skos:definition "Soils with calcium carbonate" ;
  skos:prefLabel "Calcarosols" ;
.
asc:CB
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils that are calcareous throughout the solum, or at least below the A1 horizon or to a depth of 0.2 m if the A1 horizon is only weakly developed, and do not have a clear or abrupt textural B horizon." ;
  skos:prefLabel "Calcarosolic" ;
.
asc:CC
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which the major part of the upper 0.5 m of the profile consists of materials dominated (> 50%) by halite crystals." ;
  skos:prefLabel "Halic" ;
.
asc:CD
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Other soils in which the major part of the upper 0.5 m of the solum is whole coloured." ;
  skos:prefLabel "Haplic" ;
.
asc:CE
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which the organic materials are dominated by hemic peat." ;
  skos:prefLabel "Hemic" ;
.
asc:CF
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Other soils which are dominated by organic materials to a depth of 0.5m." ;
  skos:prefLabel "Histic" ;
.
asc:CG
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with only a Bh horizon." ;
  skos:prefLabel "Humic" ;
.
asc:CH
  rdf:type soil:Order ;
  rdf:type skos:Concept ;
  skos:definition "Soils with an abrupt increase in clay" ;
  skos:prefLabel "Chromosols" ;
.
asc:CI
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a Bh/Bhs horizon." ;
  skos:prefLabel "Humic/Humosesquic" ;
.
asc:CJ
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a Bh/Bs horizon." ;
  skos:prefLabel "Humic/Sesquic" ;
.
asc:CK
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Other soils with a humose horizon." ;
  skos:prefLabel "Humose" ;
.
asc:CL
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a humose horizon and a B2 horizon in which the major part has an exchangeable Ca/Mg ratio of less than 0.1." ;
  skos:prefLabel "Humose-magnesic" ;
.
asc:CM
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a humose horizon and the major part of the B2 horizon is mottled." ;
  skos:prefLabel "Humose-mottled" ;
.
asc:CN
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a humose horizon and a strongly coherent B horizon." ;
  skos:prefLabel "Humose-parapanic" ;
.
asc:CO
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with only a Bhs horizon." ;
  skos:prefLabel "Humosesquic" ;
.
asc:CP
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which the upper 0.1m of the profile consists of more than 20% (visual estimate) of soft, finely divided carbonate, and/or has a strong effervescence with 1M HC1, and/or contains more than 8%2 (by analysis) of soft, finely divided carbonate." ;
  skos:prefLabel "Hypervescent" ;
.
asc:CQ
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a calcareous horizon containing more than 20% of mainly soft, finely divided carbonate, and 0-20% of hard calcrete fragments and/or carbonate nodules or concretions, and/or carbonate-coated gravel." ;
  skos:prefLabel "Hypercalcic" ;
.
asc:CR
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Other soils in which the major part of the upper 0.2 m of the B2 horizon has an ESP greater than 25." ;
  skos:prefLabel "Hypernatric" ;
.
asc:CS
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils of the saline playa lakes (including coastal salinas and continental playas) which are usually bare and when dry are frequently halite - or gypsum-encrusted, or with a sparse cover of halophytes such as samphires. The continental playas are infrequently inundated with fresh water, but a shallow saline ground water table is usually present in all types throughout the year, mostly within 50 cm of the surface." ;
  skos:prefLabel "Hypersalic" ;
.
asc:CU
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which some subsurface horizon within the upper 0.5 m of the solum has an ESP of 15 or greater and the major part of the upper 0.5 m of the solum is strongly acid." ;
  skos:prefLabel "Epihypersodic-epiacidic" ;
.
asc:CV
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which the carbonate is evident only as a slight to moderate effervescence (1M HCl), and/or contain less than 2% soft finely divided carbonate, and have less than 20% hard carbonate nodules or concretions." ;
  skos:prefLabel "Hypocalcic" ;
.
asc:CW
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils of intertidal flats (often colonised by mangroves) that experience regular saline tidal inundation of mostly high frequency." ;
  skos:prefLabel "Intertidal" ;
.
asc:CX
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a clear or abrupt textural B horizon and the major part of the upper 0.2 m of the B2 horizon is strongly acid." ;
  skos:prefLabel "Kurosolic" ;
.
asc:CY
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Other soils that are underlain within 0.5m of the surface by a calcrete pan; hard unweathered rock or other hard materials; or partially weathered or decomposed rock or saprolite." ;
  skos:prefLabel "Leptic" ;
.
asc:CZ
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which the organic materials directly overlie hard rock." ;
  skos:prefLabel "Lithic" ;
.
asc:D
  rdf:type soil:Family ;
  rdf:type skos:Concept ;
  skos:definition "> 0.6 m A-horizon thickness" ;
  skos:notation "D" ;
  skos:prefLabel "Very thick" ;
.
asc:DA
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a calcareous horizon containing more than 50% of hard calcrete fragments and/or carbonate nodules or concretions and/or carbonate-coated gravel." ;
  skos:prefLabel "Lithocalcic" ;
.
asc:DB
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils in which the major part of the B horizon has an exchangeable Ca/Mg ratio of less than 0.1." ;
  skos:prefLabel "Magnesic" ;
.
asc:DC
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Other soils with a manganic horizon within the solum." ;
  skos:prefLabel "Manganic" ;
.
asc:DD
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a marl layer either within or immediately below the section containing the organic materials." ;
  skos:prefLabel "Marly" ;
.
asc:DE
  rdf:type soil:Order ;
  rdf:type skos:Concept ;
  skos:definition "Structured soils" ;
  skos:prefLabel "Dermosols" ;
.
asc:DF
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a massive or weak blocky (usually > 0.05 m peds) A horizon, and there is no surface crusty horizon." ;
  skos:prefLabel "Massive" ;
.
asc:DG
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Other soils with a melacic horizon." ;
  skos:prefLabel "Melacic" ;
.
asc:DH
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a melacic horizon and a B2 horizon in which the major part has an exchangeable Ca/Mg ratio of less than 0.1." ;
  skos:prefLabel "Melacic-magnesic" ;
.
asc:DI
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a melacic horizon and the major part of the B2 horizon is mottled." ;
  skos:prefLabel "Melacic-mottled" ;
.
asc:DJ
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a melacic horizon and a strongly coherent B horizon." ;
  skos:prefLabel "Melacic-parapanic" ;
.
asc:DK
  rdf:type soil:Class ;
  rdf:type skos:Concept ;
  skos:definition "Soils with a melanic horizon." ;
  skos:prefLabel "Melanic" ;
.
asc:DL
  rdf:type soil:Class ;
