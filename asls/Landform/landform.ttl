  skos:member lf:mode-of-geomorphological-activity-BU ;
  skos:member lf:mode-of-geomorphological-activity-EA ;
  skos:member lf:mode-of-geomorphological-activity-ER ;
  skos:member lf:mode-of-geomorphological-activity-EX ;
