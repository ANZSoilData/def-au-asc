  rdfs:range soil:Family ;
.
soil:fam-type5
  rdf:type owl:ObjectProperty ;
  rdfs:domain soil:Classification ;
  rdfs:range soil:Family ;
.
soil:greatGroup
  rdf:type owl:ObjectProperty ;
  rdfs:domain soil:Classification ;
  rdfs:range soil:Class ;
.
soil:order
  rdf:type owl:ObjectProperty ;
