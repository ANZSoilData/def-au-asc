  owl:imports <http://purl.org/linked-data/registry> ;
  owl:imports ui: ;
  owl:imports sosa: ;
  skos:changeNote """Add explicit identifiers using 

CONSTRUCT {
	?c dct:identifier ?id.
}
WHERE {
	?c a skos:Collection ;
