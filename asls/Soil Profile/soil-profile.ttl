sp:coarse-fragments-distribution-D
  a skos:Concept ;
  dct:description "fragments are scattered randomly throughout the soil and may be of mixed origin." ;
  dct:identifier "http://anzsoil.org/def/au/asls/soil-profile/coarse-fragments-distribution-D"^^xsd:anyURI ;
  ui:isMemberOf sp:coarse-fragments-distribution ;
