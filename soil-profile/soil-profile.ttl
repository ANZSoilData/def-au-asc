  skos:narrower sp:field-texture-grade-FSL ;
  skos:narrower sp:field-texture-grade-HC ;
  skos:narrower sp:field-texture-grade-KS ;
  skos:narrower sp:field-texture-grade-KSCL ;
  skos:narrower sp:field-texture-grade-KSL ;
  skos:narrower sp:field-texture-grade-L ;
  skos:narrower sp:field-texture-grade-LC ;
  skos:narrower sp:field-texture-grade-LFS ;
  skos:narrower sp:field-texture-grade-LKS ;
  skos:narrower sp:field-texture-grade-LMC ;
  skos:narrower sp:field-texture-grade-LS ;
  skos:narrower sp:field-texture-grade-MC ;
